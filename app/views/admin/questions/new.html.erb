<div class="container">
  <h1>クイズ内容登録</h1>
  <%= form_with model: @question,url: admin_quiz_questions_path(@quiz.id) do |f| %>
    <%= f.text_field :body %>
    <%= f.submit"登録" %>
  <% end %>

  <% @questions.each do |list| %>
  　<%= link_to new_admin_quiz_question_choice_path(@quiz.id, list.id) do %>
      <%= list.body %>
    <% end %>
  <% end %>
</div>